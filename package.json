{
  "name": "cubic-bezier-easing",
  "version": "1.0.13",
  "description": "A simple cubic-bezier easing functions factory for KUTE.js, developed with ES6+ and based on UnitBezier",
  "main": "dist/cubic-bezier.js",
  "unpkg": "dist/cubic-bezier.min.js",
  "jsdelivr": "dist/cubic-bezier.min.js",
  "module": "dist/cubic-bezier.esm.js",
  "types": "types/index.d.ts",
  "jsnext": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "help": "rollup --help",
    "fix:js": "eslint *.js --config .eslintrc --fix --ignore-pattern '*.config.js'",
    "lint:js": "eslint *.js --config .eslintrc --ignore-pattern '*.config.js'",
    "build": "npm run lint:js && npm-run-all --parallel build-*",
    "build:ts": "tsc -d",
    "build-js": "rollup --environment FORMAT:umd,MIN:false -c",
    "build-minjs": "rollup --environment FORMAT:umd,MIN:true -c",
    "build-esm": "rollup --environment FORMAT:esm,MIN:false -c",
    "build-esmjs": "rollup --environment FORMAT:esm,MIN:true -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thednp/CubicBezier.git"
  },
  "keywords": [
    "kute.js",
    "cubic-bezier",
    "cubic-bezier-easing",
    "easing",
    "ease",
    "easing functions",
    "javascript animation engine",
    "javascript animation",
    "animations",
    "native javascript"
  ],
  "author": "thednp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thednp/CubicBezier/issues"
  },
  "homepage": "https://github.com/thednp/CubicBezier",
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-json": "^4.0.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.7.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.32.1",
    "rollup-plugin-terser": "^5.3.0",
    "typescript": "^4.4.4"
  }
}
